package stock;

import java.util.ArrayList;
import java.util.Collections;
import java.util.Comparator;

import jerarquia.Articulo;
/**
 * Ordena el stock por nombre del articulo
 * @author pablo
 *
 */
public class OrdenarNombre implements Comparator <Articulo> {

		
		OrdenarNombre(){
		}
		 @Override
		    public int compare(Articulo e1, Articulo e2) {
				return e1.getNombre().toLowerCase().compareTo(e2.getNombre().toLowerCase());

		    }
		
	}


